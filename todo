UUID=$(uuidgen)
echo "Using UUID: $UUID"
curl -X POST "http://localhost:8081/log_game_player_statistic/v1/import/$UUID" \
  -H "Content-Type: application/json" \
  --data-binary @examples/game_2024_lakers_vs_warriors_stats.json

